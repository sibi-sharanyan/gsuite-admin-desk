<form class="generalform">
    <div class="form-group">
        Street Address
        {{input type="text" class="form-control" placeholder="Enter  Street Address" value=streetAddress}}

    </div>
    <div class="form-group">
        Locality
        {{input type="text" class="form-control" placeholder="Enter Locality" value=locality}}

    </div>
    <div class="form-group">
        Region
        {{input type="text" class="form-control"  placeholder="Enter Region" value=region }}
    </div>

    <div class="form-group">
        Postal code
        {{input type="text" class="form-control"  placeholder="Enter Postal code" value = postalCode}}
    </div>

    <div class="form-group">
        Phone Number
        {{input type="text" class="form-control"  placeholder="Enter Phone Number" value = phoneno}}
    </div>
</form>

<button class="btn btn-success float-right" {{action "nextpage" }}>Next Page ></button>
<button class="btn btn-success float-left" {{action "prevpage" }}>
    &lt; Previous Page </button>